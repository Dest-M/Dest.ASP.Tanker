
@model Dest.ASP.Tester.Models.GameFieldModel;


<div>
    <div class="GameField">
        
        @for (int row = 0; row < Model.FieldHeight; row++)
        {
            
               
            @Html.Raw(@"<div class = ""GameFieldRow"">");

            for (int column = 0; column < Model.FieldWidth; column++)
            {
                await Html.RenderPartialAsync("GameFieldCell", Model.Map[row][column]);
            }
            @Html.Raw("</div>");
        }
    </div>
</div>